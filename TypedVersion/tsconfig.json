{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "preserveConstEnums": true,
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "jsx": "react",
    "baseUrl": "./",
        "paths": {
            "Utils/*": ["src/utils/*"],
            "Store/*": ["src/stores/*"]
        }
  },
  "exclude": [
    "node_modules"
  ]
}